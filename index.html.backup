<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Group Wordle â€“ Members Start Word (v7)</title>

  <!-- React + ReactDOM -->
  <script src="https://unpkg.com/react@18/umd/react.development.js"></script>
  <script src="https://unpkg.com/react-dom@18/umd/react-dom.development.js"></script>

  <!-- Babel (for JSX in the browser) -->
  <script src="https://unpkg.com/@babel/standalone/babel.min.js"></script>

  <!-- Tailwind -->
  <script src="https://cdn.tailwindcss.com"></script>

  <style>
    /* Flip animations that change colour halfway through the flip, like Wordle */
    @keyframes tile-flip-correct {
      0%   { transform: rotateX(0deg);   background-color: rgba(255,255,255,0.1); border-color: rgba(255,255,255,0.3); }
      49%  { transform: rotateX(-90deg); background-color: rgba(255,255,255,0.1); border-color: rgba(255,255,255,0.3); }
      50%  { transform: rotateX(-90deg); background-color: #16a34a;              border-color: #15803d;               }
      100% { transform: rotateX(0deg);   background-color: #16a34a;              border-color: #15803d;               }
    }

    @keyframes tile-flip-present {
      0%   { transform: rotateX(0deg);   background-color: rgba(255,255,255,0.1); border-color: rgba(255,255,255,0.3); }
      49%  { transform: rotateX(-90deg); background-color: rgba(255,255,255,0.1); border-color: rgba(255,255,255,0.3); }
      50%  { transform: rotateX(-90deg); background-color: #eab308;              border-color: #ca8a04;               }
      100% { transform: rotateX(0deg);   background-color: #eab308;              border-color: #ca8a04;               }
    }

    @keyframes tile-flip-absent {
      0%   { transform: rotateX(0deg);   background-color: rgba(255,255,255,0.1); border-color: rgba(255,255,255,0.3); }
      49%  { transform: rotateX(-90deg); background-color: rgba(255,255,255,0.1); border-color: rgba(255,255,255,0.3); }
      50%  { transform: rotateX(-90deg); background-color: #374151;              border-color: #111827;               }
      100% { transform: rotateX(0deg);   background-color: #374151;              border-color: #111827;               }
    }

    .tile-flip-correct {
      animation: tile-flip-correct 600ms ease forwards;
      transform-origin: center bottom;
      backface-visibility: hidden;
    }
    .tile-flip-present {
      animation: tile-flip-present 600ms ease forwards;
      transform-origin: center bottom;
      backface-visibility: hidden;
    }
    .tile-flip-absent {
      animation: tile-flip-absent 600ms ease forwards;
      transform-origin: center bottom;
      backface-visibility: hidden;
    }

    /* Shake animation for invalid word / too short */
    @keyframes row-shake {
      0%, 100% { transform: translateX(0); }
      15%, 45%, 75% { transform: translateX(-6px); }
      30%, 60%, 90% { transform: translateX(6px); }
    }
    .row-shake {
      animation: row-shake 400ms ease;
    }

    /* Win bounce animation when the correct word is found */
    @keyframes row-win {
      0%   { transform: translateY(0);   }
      30%  { transform: translateY(-8px);}
      60%  { transform: translateY(4px); }
      100% { transform: translateY(0);   }
    }
    .row-win {
      animation: row-win 600ms ease-out;
    }
  </style>
</head>
<body class="bg-gray-900 text-white">
  <div id="root"></div>

  <script type="text/babel">

    const API_BASE = "/.netlify/functions";

    async function fetchJson(url, options) {
      const res = await fetch(url, options);
      if (!res.ok) throw new Error(`HTTP ${res.status}`);
      return res.json();
    }

    const LOGO_SRC = "data:image/png;base64,";

    const { useState, useEffect, useCallback } = React;

    const WORDLIST_URL =
      "https://raw.githubusercontent.com/tabatkins/wordle-list/main/words";

    const KEYBOARD_ROWS = [
      ["Q","W","E","R","T","Y","U","I","O","P"],
      ["A","S","D","F","G","H","J","K","L"],
      ["Z","X","C","V","B","N","M"]
    ];

    const FLIP_DURATION = 350;  // ms each tile flip (snappier, Wordle-ish)
    const FLIP_STAGGER = 150;   // ms between tiles (Wordle-style)

    const isPlural = (word, allowedSet) => {
      if (!word || word.length !== 5) return false;
      if (!word.endsWith("S")) return false;
      if (!allowedSet) return false;
      const root = word.slice(0, -1);
      return allowedSet.has(root);
    };

    const getDailyIndex = (length, dateStr) => {
      if (!length) return 0;
      const s = dateStr || getAustralianDate();
      let seed = 0;
      for (let i = 0; i < s.length; i++) {
        seed = (seed * 31 + s.charCodeAt(i)) >>> 0;
      }
      return seed % length;
    };

    const getGuessStatuses = (guess, targetWord) => {
      const statuses = [];
      const targetChars = targetWord.split("");
      const guessChars = guess.split("");

      for (let i = 0; i < guess.length; i++) {
        if (guessChars[i] === targetChars[i]) {
          statuses[i] = "correct";
          targetChars[i] = "";
        }
      }

      for (let i = 0; i < guess.length; i++) {
        if (statuses[i] !== "correct") {
          const char = guessChars[i];
          const targetIndex = targetChars.indexOf(char);
          if (targetIndex > -1) {
            statuses[i] = "present";
            targetChars[targetIndex] = "";
          } else {
            statuses[i] = "absent";
          }
        }
      }
      return statuses;
    };

    function TabButton({ active, onClick, children }) {
      return (
        <button
          onClick={onClick}
          className={
            'flex-1 py-3 rounded-lg font-semibold flex items-center justify-center gap-2 transition ' +
            (active ? 'bg-white text-purple-900' : 'bg-purple-800 text-white hover:bg-purple-700')
          }
        >
          {children}
        </button>
      );
    }

    
    function getLetterColor(status) {
      switch (status) {
        case "correct":
          return "bg-green-600 border-green-700";
        case "present":
          return "bg-yellow-500 border-yellow-600";
        case "absent":
          return "bg-gray-700 border-gray-900";
        default:
          return "bg-white/10 border-white/30";
      }
    }

function GroupWordleGame() {
      const [view, setView] = useState("game");

      const [playerName, setPlayerName] = useState("");
      const [isPlaying, setIsPlaying] = useState(false);
      const [currentGuess, setCurrentGuess] = useState("");
      const [guesses, setGuesses] = useState([]);
      const [gameOver, setGameOver] = useState(false);
      const [message, setMessage] = useState("");
      const [keyboardStatus, setKeyboardStatus] = useState({});
      const [todayWord, setTodayWord] = useState("");
      const [todayDate, setTodayDate] = useState("");
      const [startWord, setStartWord] = useState("");
      const [startWordOwner, setStartWordOwner] = useState("");

      const [wordListLoaded, setWordListLoaded] = useState(false);
      const [allowedWords, setAllowedWords] = useState(null);
      const [loadingError, setLoadingError] = useState(null);

      // per-member start words: { memberName: [WORD1, WORD2, ...] }
      const [memberStartWords, setMemberStartWords] = useState({});
      const [newStartMemberName, setNewStartMemberName] = useState("");
      const [newStartWord, setNewStartWord] = useState("");

      const [revealRowIndex, setRevealRowIndex] = useState(null);
      const [shakeRowIndex, setShakeRowIndex] = useState(null);
      const [isRevealing, setIsRevealing] = useState(false);
      const [winningRowIndex, setWinningRowIndex] = useState(null);

      const [scores, setScores] = useState({});
      const [dailyScores, setDailyScores] = useState({});
      const [dailyPlayers, setDailyPlayers] = useState([]);

      const [isAdmin, setIsAdmin] = useState(false);
      const [adminPassword, setAdminPassword] = useState("");

      const getAustralianDate = () => {
        const now = new Date();
        const aestOffset = 10 * 60; // minutes
        const utc = now.getTime() + now.getTimezoneOffset() * 60000;
        const aestTime = new Date(utc + aestOffset * 60000);
        return aestTime.toISOString().split("T")[0];
      };

      // Load dictionary
      useEffect(() => {
        const loadWords = async () => {
          try {
            const cached = localStorage.getItem("gw_wordlist_v1");
            let rawWords;

            if (cached) {
              rawWords = JSON.parse(cached);
            } else {
              const resp = await fetch(WORDLIST_URL);
              const text = await resp.text();
              rawWords = text
                .split(/\r?\n/)
                .map(w => w.trim().toUpperCase())
                .filter(w => /^[A-Z]{5}$/.test(w));
            }

            // Build a non-plural master list for all use-cases
            const baseSet = new Set(rawWords);
            const nonPluralWords = rawWords.filter(w => !isPlural(w, baseSet));

            localStorage.setItem("gw_wordlist_v1", JSON.stringify(nonPluralWords));

            setAllowedWords(new Set(nonPluralWords));
            setWordListLoaded(true);
          } catch (err) {
            console.error("Error loading word list", err);
            setLoadingError("Could not load the word list. Check your internet connection and refresh.");
          }
        };

        loadWords();
      }, []);

      // Load per-member start words (shared via API, fallback to localStorage/default)
      useEffect(() => {
        const loadMemberStartWords = async () => {
          // Try server first
          try {
            const data = await fetchJson(`${API_BASE}/config`);
            if (data && data.memberStartWords && typeof data.memberStartWords === "object") {
              setMemberStartWords(data.memberStartWords);
              localStorage.setItem(
                "gw_member_start_words_v1",
                JSON.stringify(data.memberStartWords)
              );
              return;
            }
          } catch (err) {
            console.error("Error loading member start words from API", err);
          }

          // Fallback: localStorage
          const stored = localStorage.getItem("gw_member_start_words_v1");
          if (stored) {
            try {
              const parsed = JSON.parse(stored);
              if (parsed && typeof parsed === "object") {
                setMemberStartWords(parsed);
                return;
              }
            } catch (_) {}
          }

          // Default: one group bucket if nothing set
          setMemberStartWords({
            Group: ["ABOUT", "FORGE", "REACT", "PRANK", "CRANE"]
          });
        };

        loadMemberStartWords();
      }, []);

      // Persist member start words to both localStorage and server
      useEffect(() => {
        if (!memberStartWords || typeof memberStartWords !== "object") return;

        localStorage.setItem("gw_member_start_words_v1", JSON.stringify(memberStartWords));

        (async () => {
          try {
            await fetchJson(`${API_BASE}/config`, {
              method: "POST",
              headers: { "Content-Type": "application/json" },
              body: JSON.stringify({ memberStartWords })
            });
          } catch (err) {
            console.error("Error saving member start words to API", err);
          }
        })();
      }, [memberStartWords]);

      // Once dictionary is loaded, decide today's word, then load scores & daily players from API
      useEffect(() => {
        if (!wordListLoaded || !allowedWords) return;

        const today = getAustralianDate();
        setTodayDate(today);

        const allWords = Array.from(allowedWords);
        let candidateWords = allWords.filter(w => !isPlural(w, allowedWords));
        if (candidateWords.length === 0) candidateWords = allWords;

        const dayNumber = Math.floor(Date.now() / 86400000);
        const index = dayNumber % candidateWords.length;
        const secret = candidateWords[index];
        setTodayWord(secret);

        (async () => {
          try {
            const data = await fetchJson(
              `${API_BASE}/status?date=${encodeURIComponent(today)}`
            );
            setDailyPlayers(data.dailyPlayers || []);
            setDailyScores(data.dailyScores || {});
            setScores(data.allScores || {});
          } catch (err) {
            console.error("Error loading scores from API", err);
            setDailyPlayers([]);
            setDailyScores({});
            setScores({});
          }
        })();
      }, [wordListLoaded, allowedWords]);

      const updateKeyboardStatus = useCallback((guess, statuses) => {
        setKeyboardStatus(prevStatus => {
          const newStatus = { ...prevStatus };
          for (let i = 0; i < guess.length; i++) {
            const letter = guess[i];
            const status = statuses[i];
            const current = newStatus[letter];
            const currentPriority =
              current === "correct" ? 3 :
              current === "present" ? 2 :
              current === "absent" ? 1 : 0;
            const newPriority =
              status === "correct" ? 3 :
              status === "present" ? 2 :
              status === "absent" ? 1 : 0;

            if (newPriority > currentPriority) {
              newStatus[letter] = status;
            }
          }
          return newStatus;
        });
      }, []);

      const recordScore = (attempts, success) => {
        const name = playerName.trim();
        if (!name || !todayDate) return;

        // Optimistic local update so UI feels instant
        setDailyScores(prevDaily => {
          const existing = prevDaily[name];
          const best = existing ? Math.min(existing, attempts) : attempts;
          return { ...prevDaily, [name]: best };
        });

        setScores(prevScores => {
          const total = (prevScores[name] || 0) + attempts;
          return { ...prevScores, [name]: total };
        });

        // Save to DB then refresh from server
        (async () => {
          try {
            await fetchJson(`${API_BASE}/submit`, {
              method: "POST",
              headers: { "Content-Type": "application/json" },
              body: JSON.stringify({
                date: todayDate,
                playerName: name,
                attempts,
                success
              })
            });

            const data = await fetchJson(
              `${API_BASE}/status?date=${encodeURIComponent(todayDate)}`
            );
            setDailyPlayers(data.dailyPlayers || []);
            setDailyScores(data.dailyScores || {});
            setScores(data.allScores || {});
          } catch (err) {
            console.error("Error saving score", err);
          }
        })();
      };


      const submitGuess = useCallback(() => {
        if (gameOver || !todayWord || !isPlaying || isRevealing) return;

        const guess = currentGuess.toUpperCase();
        const currentRow = guesses.length;

        if (guess.length !== 5) {
          setMessage("Word must be 5 letters");
          setShakeRowIndex(currentRow);
          setTimeout(() => setShakeRowIndex(null), 450);
          return;
        }

        if (allowedWords && isPlural(guess, allowedWords)) {
          setMessage("Plurals not allowed");
          setShakeRowIndex(currentRow);
          setTimeout(() => setShakeRowIndex(null), 450);
          return;
        }

        if (!allowedWords || !allowedWords.has(guess)) {
          setMessage("Not in word list");
          setShakeRowIndex(currentRow);
          setTimeout(() => setShakeRowIndex(null), 450);
          return;
        }

        const statuses = getGuessStatuses(guess, todayWord);

        const newGuesses = [...guesses, guess];
        const newRowIndex = newGuesses.length - 1;
        setGuesses(newGuesses);
        setCurrentGuess("");
        updateKeyboardStatus(guess, statuses);
        setRevealRowIndex(newRowIndex);
        setIsRevealing(true);

        const totalRevealTime = FLIP_DURATION + FLIP_STAGGER * (guess.length - 1) + 200;

        setTimeout(() => {
          setIsRevealing(false);
          setRevealRowIndex(null);
        }, totalRevealTime);

        const attemptsUsed = newGuesses.length;

        if (guess === todayWord) {
          recordScore(attemptsUsed, true);
          setGameOver(true);
          setMessage(
            `ðŸŽ‰ Correct! You got it in ${attemptsUsed} ${attemptsUsed === 1 ? "try" : "tries"}!`
          );
          // Trigger winning row bounce after reveal finishes
          setTimeout(() => {
            setWinningRowIndex(newRowIndex);
          }, totalRevealTime);
        } else if (newGuesses.length >= 6) {
          recordScore(7, false);
          setGameOver(true);
          setMessage(`Game Over! The word was ${todayWord}`);
        } else {
          setMessage("");
        }
      }, [
        gameOver,
        todayWord,
        isPlaying,
        isRevealing,
        currentGuess,
        allowedWords,
        guesses,
        updateKeyboardStatus
      ]);

      const handleKeyPress = useCallback((key) => {
        if (!isPlaying || gameOver || isRevealing) return;

        if (key === "ENTER") {
          submitGuess();
        } else if (key === "BACK") {
          setCurrentGuess(prev => prev.slice(0, -1));
        } else if (/^[A-Z]$/.test(key)) {
          setCurrentGuess(prev => {
            if (prev.length >= 5) return prev;
            return prev + key;
          });
        }
      }, [isPlaying, gameOver, isRevealing, submitGuess]);

      // Physical keyboard support
      useEffect(() => {
        const onKeyDown = (e) => {
          if (!isPlaying || gameOver || isRevealing) return;
          const key = e.key;
          if (/^[a-zA-Z]$/.test(key)) {
            e.preventDefault();
            handleKeyPress(key.toUpperCase());
          } else if (key === "Enter") {
            e.preventDefault();
            handleKeyPress("ENTER");
          } else if (key === "Backspace") {
            e.preventDefault();
            handleKeyPress("BACK");
          }
        };
        window.addEventListener("keydown", onKeyDown);
        return () => window.removeEventListener("keydown", onKeyDown);
      }, [isPlaying, gameOver, isRevealing, handleKeyPress]);

      const startGame = async () => {
        const name = playerName.trim();
        if (!name) return;

        if (!wordListLoaded || !todayWord) {
          setMessage("Still loading dictionaryâ€¦ please wait a moment.");
          return;
        }

        const today = getAustralianDate();

        try {
          const data = await fetchJson(`${API_BASE}/start`, {
            method: "POST",
            headers: { "Content-Type": "application/json" },
            body: JSON.stringify({ date: today, playerName: name })
          });

          if (!data.allowed) {
            setDailyPlayers(data.dailyPlayers || []);
            setMessage(name + " has already played today.");
            return;
          }

          if (Array.isArray(data.dailyPlayers)) {
            setDailyPlayers(data.dailyPlayers);
          }
        } catch (err) {
          console.error("Error calling start API", err);
          setMessage("Error talking to the server. Please try again.");
          return;
        }

        // Build candidate list of { member, word }
        let pairs = [];
        Object.entries(memberStartWords).forEach(([member, words]) => {
          if (!Array.isArray(words)) return;
          words.forEach((w) => {
            const upper = String(w).toUpperCase().trim();
            if (!/^[A-Z]{5}$/.test(upper)) return;
            if (allowedWords && !allowedWords.has(upper)) return;
            if (allowedWords && isPlural(upper, allowedWords)) return;
            pairs.push({ member, word: upper });
          });
        });

        if (allowedWords && pairs.length === 0) {
          const allWords = Array.from(allowedWords);
          let candidateWords = allWords.filter((w) => !isPlural(w, allowedWords));
          if (candidateWords.length === 0) candidateWords = allWords;
          const idx = getDailyIndex(candidateWords.length, today);
          pairs.push({ member: "Group", word: candidateWords[idx] });
        }

        let chosen;
        if (pairs.length) {
          const idx = getDailyIndex(pairs.length, today);
          chosen = pairs[idx];
        } else {
          chosen = { member: "Group", word: "ABOUT" };
        }

        setStartWord(chosen.word);
        setStartWordOwner(chosen.member);

        const statuses = getGuessStatuses(chosen.word, todayWord);
        const initialGuesses = [chosen.word];
        setGuesses(initialGuesses);
        updateKeyboardStatus(chosen.word, statuses);

        setIsPlaying(true);
        setGameOver(false);
        setCurrentGuess("");
        setMessage("");
        setShakeRowIndex(null);
        setWinningRowIndex(null);

        setRevealRowIndex(0);
        setIsRevealing(true);
        const totalRevealTime =
          FLIP_DURATION + FLIP_STAGGER * (chosen.word.length - 1) + 200;
        setTimeout(() => {
          setIsRevealing(false);
          setRevealRowIndex(null);
        }, totalRevealTime);
      };

      const resetGame = () => {
        setIsPlaying(false);
        setGameOver(false);
        setGuesses([]);
        setCurrentGuess("");
        setMessage("");
        setStartWord("");
        setStartWordOwner("");
        setKeyboardStatus({});
        setView("game");
      };

const addMemberStartWord = () => {
        const member = newStartMemberName.trim();
        const w = newStartWord.toUpperCase().trim();
        if (!member || w.length !== 5 || !/^[A-Z]{5}$/.test(w)) return;
        if (allowedWords && isPlural(w, allowedWords)) {
          setMessage("Plurals not allowed for start words");
          return;
        }

        setMemberStartWords(prev => {
          const existing = prev[member] || [];
          if (existing.includes(w)) return prev;
          return {
            ...prev,
            [member]: [...existing, w]
          };
        });
        setNewStartWord("");
      };

      const removeMemberStartWord = (member, word) => {
        setMemberStartWords(prev => {
          const existing = prev[member] || [];
          const updatedMemberWords = existing.filter(w => w !== word);
          const updated = { ...prev };
          if (updatedMemberWords.length) {
            updated[member] = updatedMemberWords;
          } else {
            delete updated[member];
          }
          return updated;
        });
      };

      const getTodayResults = () => {
        return Object.entries(dailyScores)
          .map(([name, score]) => ({ name, score }))
          .sort((a, b) => a.score - b.score);
      };

      const getLeaderboard = () => {
        return Object.entries(scores)
          .map(([name, score]) => ({ name, score }))
          .sort((a, b) => a.score - b.score);
      };

      const checkAdminPassword = () => {
        if (adminPassword === "admin123") {
          setIsAdmin(true);
          setMessage("Admin access granted");
        } else {
          setMessage("Incorrect password");
        }
      };

      if (loadingError) {
        return (
          <div className="min-h-screen flex items-center justify-center bg-gray-900">
            <div className="max-w-md w-full bg-red-900/40 border border-red-500 rounded-xl p-6 text-center">
              <h1 className="text-2xl font-bold text-white mb-4">Problem Loading Word List</h1>
              <p className="text-red-100 mb-4">
                {loadingError}
              </p>
              <p className="text-red-100 text-sm">
                You need an internet connection the first time to download the dictionary.
              </p>
            </div>
          </div>
        );
      }

      if (!wordListLoaded || !todayWord) {
        return (
          <div className="min-h-screen flex items-center justify-center bg-gray-900">
            <div className="flex flex-col items-center gap-4">
              <div className="w-10 h-10 border-4 border-purple-400 border-t-transparent rounded-full animate-spin"></div>
              <p className="text-purple-100 text-lg font-semibold">
                Loading dictionary and today&apos;s wordâ€¦
              </p>
            </div>
          </div>
        );
      }

      return (
        <div className="min-h-screen bg-gradient-to-br from-indigo-900 via-purple-900 to-pink-900 p-4">
          <div className="max-w-3xl mx-auto">
            <div className="text-center mb-6 pt-4">
              <div className="flex flex-col items-center gap-3 mb-4">
                <img
                  src={LOGO_SRC}
                  alt="Group Wordle logo"
                  className="w-32 h-32 md:w-40 md:h-40 object-contain drop-shadow-lg"
                />
              </div>
              <h1 className="text-4xl md:text-5xl font-bold text-white mb-2">
                Group Wordle
              </h1>
              <div className="mt-3 bg-white/20 backdrop-blur-sm rounded-lg p-4 inline-block">
                <p className="text-purple-200 text-sm mb-1">Word of the Day</p>
                <p className="text-3xl font-bold text-white tracking-widest">
                  {"*****"}
                </p>
                <p className="text-purple-200 text-sm mt-1">{todayDate} (AEST)</p>
                {startWord && (
                  <p className="text-purple-100 text-xs mt-2">
                    Today&apos;s starting word was:{" "}
                    <span className="font-bold">{startWord}</span>
                    {startWordOwner && (
                      <>
                        {" "}from{" "}
                        <span className="font-bold underline">{startWordOwner}</span>
                      </>
                    )}
                  </p>
                )}
              </div>
            </div>

            <div className="flex gap-2 mb-6">
              <TabButton active={view === "game"} onClick={() => setView("game")}>
                <span>ðŸ“…</span> <span>Play</span>
              </TabButton>
              <TabButton active={view === "today"} onClick={() => setView("today")}>
                <span>âœ…</span> <span>Today</span>
              </TabButton>
              <TabButton active={view === "leaderboard"} onClick={() => setView("leaderboard")}>
                <span>ðŸ†</span> <span>Leaders</span>
              </TabButton>
              <TabButton active={view === "admin"} onClick={() => setView("admin")}>
                <span>ðŸ‘¥</span> <span>Admin</span>
              </TabButton>
            </div>

            {message && (
              <div className="mb-4 p-4 bg-white/20 backdrop-blur-sm rounded-lg text-white text-center font-semibold">
                {message}
              </div>
            )}

            {view === "game" && (
              <div className="bg-white/10 backdrop-blur-md rounded-2xl p-8 shadow-2xl">
                {!isPlaying ? (
                  <div className="space-y-4">
                    <h2 className="text-2xl font-bold text-white text-center mb-6">
                      Enter Your Name
                    </h2>
                    <input
                      type="text"
                      value={playerName}
                      onChange={e => setPlayerName(e.target.value)}
                      placeholder="Your name"
                      className="w-full p-4 rounded-lg text-lg text-center font-semibold bg-white/20 text-white placeholder-purple-200 border-2 border-white/30 focus:border-white focus:outline-none"
                      onKeyDown={e => e.key === "Enter" && startGame()}
                    />
                    <button
                      onClick={startGame}
                      className="w-full py-4 bg-gradient-to-r from-pink-500 to-purple-500 text-white rounded-lg font-bold text-lg hover:from-pink-600 hover:to-purple-600 transition"
                    >
                      Start Game (random member word first)
                    </button>
                    <p className="text-xs text-purple-200 text-center mt-2">
                      Each member (name) can only play once per day. Once started, just type â€“ letters appear in the current row; Enter submits, Backspace deletes.
                    </p>
                  </div>
                ) : (
                  <div>
                    <div className="flex justify-between items-center mb-6">
                      <h2 className="text-2xl font-bold text-white">
                        Playing: {playerName}
                      </h2>
                      <span className="text-purple-200 text-lg">
                        Attempt {guesses.length}/6
                      </span>
                    </div>

                    <div className="space-y-2 mb-6">
                      {[...Array(6)].map((_, i) => {
                        const guess = guesses[i];
                        const isCurrentRow = i === guesses.length && !gameOver;
                        const displayGuess = isCurrentRow ? currentGuess : (guess || "");

                        let statuses = Array(5).fill(null);
                        if (guess) {
                          statuses = getGuessStatuses(guess, todayWord);
                        }

                        const rowIsShaking = shakeRowIndex === i;
                        const rowIsRevealing = revealRowIndex === i && !!guess;
                        const rowIsWinning = winningRowIndex === i;

                        return (
                          <div
                            key={i}
                            className={
                              "flex gap-2 justify-center " +
                              (rowIsShaking ? "row-shake " : "") +
                              (rowIsWinning ? " row-win " : "") +
                              (isCurrentRow && !gameOver ? "cursor-text" : "")
                            }
                          >
                            {[...Array(5)].map((_, j) => {
                              const letter = displayGuess[j] || "";
                              const status = statuses[j];

                              let colorClass;
                              let flipClass = "";
                              const delayStyle = rowIsRevealing && letter
                                ? { animationDelay: `${j * FLIP_STAGGER}ms` }
                                : {};

                              if (rowIsRevealing && letter) {
                                // During reveal: neutral base colour, colour comes from animation mid-flip
                                colorClass = "bg-white/10 border-white/30";
                                if (status === "correct") flipClass = " tile-flip-correct";
                                else if (status === "present") flipClass = " tile-flip-present";
                                else flipClass = " tile-flip-absent";
                              } else {
                                // Not currently revealing: show static colours
                                if (status) {
                                  colorClass = getLetterColor(status);
                                } else {
                                  colorClass = letter
                                    ? "bg-white/40 border-white/50"
                                    : "bg-white/10 border-white/30";
                                }
                              }

                              return (
                                <div
                                  key={j}
                                  style={delayStyle}
                                  className={
                                    `w-14 h-14 ${colorClass} rounded-lg flex items-center justify-center ` +
                                    `text-2xl font-bold text-white border-2 transition-all` +
                                    flipClass
                                  }
                                >
                                  {letter}
                                </div>
                              );
                            })}
                          </div>
                        );
                      })}
                    </div>

                    <p className="text-center text-xs text-purple-200 mb-4">
                      Type letters on your keyboard to fill the current row. Press Enter to submit, or use the on-screen keys.
                    </p>

                    <div className="space-y-2 mt-2">
                      {KEYBOARD_ROWS.map((row, ri) => (
                        <div key={ri} className="flex gap-1 justify-center">
                          {ri === 2 && (
                            <button
                              onClick={() => handleKeyPress("ENTER")}
                              disabled={gameOver || currentGuess.length !== 5 || isRevealing}
                              className="px-3 h-12 bg-gray-400 hover:bg-gray-300 disabled:bg-gray-600 disabled:opacity-50 text-white text-xs font-bold rounded transition"
                            >
                              ENTER
                            </button>
                          )}
                          {row.map(key => {
                            const status = keyboardStatus[key];
                            let bgColor = "bg-gray-400 hover:bg-gray-300";
                            if (status === "correct") bgColor = "bg-green-600 hover:bg-green-500";
                            else if (status === "present") bgColor = "bg-yellow-500 hover:bg-yellow-400";
                            else if (status === "absent") bgColor = "bg-gray-700 hover:bg-gray-600";

                            return (
                              <button
                                key={key}
                                onClick={() => handleKeyPress(key)}
                                disabled={gameOver || isRevealing}
                                className={
                                  `w-10 h-12 ${bgColor} disabled:opacity-50 text-white font-bold rounded transition`
                                }
                              >
                                {key}
                              </button>
                            );
                          })}
                          {ri === 2 && (
                            <button
                              onClick={() => handleKeyPress("BACK")}
                              disabled={gameOver || currentGuess.length === 0 || isRevealing}
                              className="px-3 h-12 bg-gray-400 hover:bg-gray-300 disabled:bg-gray-600 disabled:opacity-50 text-white text-xs font-bold rounded transition"
                            >
                              â†
                            </button>
                          )}
                        </div>
                      ))}
                    </div>

                    {gameOver && (
                      <button
                        onClick={resetGame}
                        className="w-full mt-6 py-4 bg-gradient-to-r from-blue-500 to-indigo-500 text-white rounded-lg font-bold text-lg hover:from-blue-600 hover:to-indigo-600 transition"
                      >
                        Play Again (New Player)
                      </button>
                    )}
                  </div>
                )}
              </div>
            )}

            {view === "today" && (
              <div className="bg-white/10 backdrop-blur-md rounded-2xl p-8 shadow-2xl">
                <h2 className="text-3xl font-bold text-white mb-6">Today&apos;s Results</h2>
                {getTodayResults().length > 0 ? (
                  <div className="space-y-2 max-h-96 overflow-y-auto">
                    {getTodayResults().map((player, i) => (
                      <div
                        key={player.name}
                        className="bg-white/20 backdrop-blur-sm rounded-lg p-4 flex justify-between items-center"
                      >
                        <div className="flex items-center gap-4">
                          <span className="text-2xl font-bold text-white w-8">
                            #{i + 1}
                          </span>
                          <span className="text-lg font-semibold text-white">
                            {player.name}
                          </span>
                        </div>
                        <span className="text-xl font-bold text-yellow-400">
                          {player.score} {player.score === 1 ? "try" : "tries"}
                        </span>
                      </div>
                    ))}
                  </div>
                ) : (
                  <p className="text-purple-200 text-center py-4">
                    No games played yet today
                  </p>
                )}
              </div>
            )}

            {view === "leaderboard" && (
              <div className="bg-white/10 backdrop-blur-md rounded-2xl p-8 shadow-2xl">
                <h2 className="text-3xl font-bold text-white mb-6">All-Time Leaderboard</h2>
                {getLeaderboard().length > 0 ? (
                  <div className="space-y-2 max-h-96 overflow-y-auto">
                    {getLeaderboard().map((player, i) => (
                      <div
                        key={player.name}
                        className={
                          `rounded-lg p-4 flex justify-between items-center ` +
                          (i === 0
                            ? 'bg-gradient-to-r from-yellow-500/30 to-yellow-600/30 border-2 border-yellow-400'
                            : 'bg-white/10')
                        }
                      >
                        <div className="flex items-center gap-4">
                          <span
                            className={
                              `text-2xl font-bold w-8 ` +
                              (i === 0 ? 'text-yellow-400' : 'text-white')
                            }
                          >
                            #{i + 1}
                          </span>
                          <span className="text-lg font-semibold text-white">
                            {player.name}
                          </span>
                        </div>
                        <span
                          className={
                            `text-xl font-bold ` +
                            (i === 0 ? 'text-yellow-400' : 'text-purple-200')
                          }
                        >
                          {player.score} pts
                        </span>
                      </div>
                    ))}
                  </div>
                ) : (
                  <p className="text-purple-200 text-center py-4">
                    No scores recorded yet
                  </p>
                )}
              </div>
            )}

            {view === "admin" && (
              <div className="bg-white/10 backdrop-blur-md rounded-2xl p-8 shadow-2xl">
                <h2 className="text-3xl font-bold text-white mb-6">Admin Panel</h2>
                {!isAdmin ? (
                  <div className="space-y-4">
                    <input
                      type="password"
                      value={adminPassword}
                      onChange={e => setAdminPassword(e.target.value)}
                      placeholder="Enter admin password"
                      className="w-full p-4 rounded-lg text-lg bg-white/20 text-white placeholder-purple-200 border-2 border-white/30 focus:border-white focus:outline-none"
                      onKeyDown={e => e.key === "Enter" && checkAdminPassword()}
                    />
                    <button
                      onClick={checkAdminPassword}
                      className="w-full py-4 bg-gradient-to-r from-purple-500 to-pink-500 text-white rounded-lg font-bold text-lg hover:from-purple-600 hover:to-pink-600 transition"
                    >
                      Login
                    </button>
                    <p className="text-purple-200 text-sm text-center">
                      Default password: admin123
                    </p>
                  </div>
                ) : (
                  <div className="space-y-6">
                    <div>
                      <h3 className="text-xl font-bold text-white mb-4">
                        Member Start Words
                      </h3>
                      <p className="text-purple-200 text-sm mb-2">
                        Each member can have multiple start words. When a game begins, a random word is selected from all members&apos; lists and we show whose word it was. Plurals are not allowed.
                      </p>
                      <div className="flex flex-col md:flex-row gap-2 mb-3">
                        <input
                          type="text"
                          value={newStartMemberName}
                          onChange={e => setNewStartMemberName(e.target.value)}
                          placeholder="Member name"
                          className="flex-1 p-3 rounded-lg text-lg bg-white/20 text-white placeholder-purple-200 border-2 border-white/30 focus:border-white focus:outline-none"
                        />
                        <input
                          type="text"
                          value={newStartWord}
                          onChange={e =>
                            setNewStartWord(
                              e.target.value.toUpperCase().replace(/[^A-Z]/g, "").slice(0, 5)
                            )
                          }
                          maxLength={5}
                          placeholder="5-letter word"
                          className="w-40 p-3 rounded-lg text-lg bg-white/20 text-white placeholder-purple-200 border-2 border-white/30 focus:border-white focus:outline-none uppercase text-center"
                        />
                        <button
                          onClick={addMemberStartWord}
                          className="px-4 py-3 bg-green-500 text-white rounded-lg font-bold hover:bg-green-600 transition"
                        >
                          Add
                        </button>
                      </div>
                      {Object.keys(memberStartWords).length === 0 ? (
                        <p className="text-purple-200 text-sm">
                          No member start words yet. Add some above.
                        </p>
                      ) : (
                        <div className="max-h-64 overflow-y-auto space-y-4">
                          {Object.entries(memberStartWords).map(([member, words]) => (
                            <div key={member} className="bg-white/10 rounded-lg p-3">
                              <div className="flex justify-between items-center mb-2">
                                <span className="font-semibold text-white">{member}</span>
                              </div>
                              <div className="flex flex-wrap gap-2">
                                {words.map(word => (
                                  <div
                                    key={word}
                                    className="flex items-center bg-white/20 rounded px-2 py-1 text-xs"
                                  >
                                    <span className="font-mono tracking-[0.25em] mr-1">
                                      {word}
                                    </span>
                                    <button
                                      onClick={() => removeMemberStartWord(member, word)}
                                      className="text-red-300 hover:text-red-100 text-xs"
                                    >
                                      âœ•
                                    </button>
                                  </div>
                                ))}
                              </div>
                            </div>
                          ))}
                        </div>
                      )}
                    </div>

                    <div className="bg-white/20 rounded-lg p-4">
                      <p className="text-purple-200">
                        Today&apos;s word:{" "}
                        <span className="text-white font-bold">{todayWord}</span>
                      </p>
                      <p className="text-purple-200">
                        Current session start word:{" "}
                        <span className="text-white font-bold">
                          {startWord || "(not chosen yet this session)"}
                        </span>
                        {startWordOwner && (
                          <>
                            {" "}from{" "}
                            <span className="text-white font-bold underline">
                              {startWordOwner}
                            </span>
                          </>
                        )}
                      </p>
                      <p className="text-purple-200">
                        Date:{" "}
                        <span className="text-white font-bold">
                          {todayDate} (AEST)
                        </span>
                      </p>
                    </div>
                  </div>
                )}
              </div>
            )}
          </div>
        </div>
      );
    }

    ReactDOM.createRoot(document.getElementById("root")).render(<GroupWordleGame />);
  </script>
</body>
</html>
